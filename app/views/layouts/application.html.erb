<!-- jQquery validation -->
<script type="text/javascript">
    $("#saveListingBtn").click(function() {
        $("#new_office").validate({
            rules: {
                "office[contact_email]": {required: true, email: true},
                "office[title]": {required: true}
            },
            messages: {
                "office[contact_email]": "Please provide a valid email",
                "office[title]": "Please provide a title for your listing"
            },
            errorPlacement: function(error, element) {
                element.after(error);
                element.css('border', 'solid red 2px');
            }
        });
        _gaq.push(['_trackEvent', 'SaveListing', 'User saves a listing']);
    });

    $("#newListingBtn").click(function() {
        _gaq.push(['_trackEvent', 'NewListing', 'User clicks on Add Listing']);
    });

    $("#seeListingBtn").click(function() {
        _gaq.push(['_trackEvent', 'SeeListings', 'User clicks on See Listing']);
    });

</script>


