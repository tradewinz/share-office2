<!doctype html>

<html>

  

  <head>

    <script src="https://app.divshot.com/js/jquery.min.js"></script>

    <script src="https://app.divshot.com/js/bootstrap.min.js"></script>


    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">

    <link rel="stylesheet" href="https://app.divshot.com/css/bootstrap.css">

    <link rel="stylesheet" href="https://app.divshot.com/css/bootstrap-responsive.css"> 

  </head>

  

  <body>

    <div class="container-fluid">      

      <div class="container">

        <div class="row">

          <div class="span6">

            <img src="https://s3-us-west-2.amazonaws.com/officespace/floorplan.png"> 

          </div>

          <div class="span6">

            <div class="hero-unit">

              <h2>See what your extra work space is worth</h2>

              <p>Tell us about your space, and we'll tell you how much it's worth.</p>

              <%= form_for(@rentestimate) do |f| %>
                
                <!--%= f.text_field :industry --> 
                <input type="text" name="rentestimate[industry]" placeholder="Your Industry" class="input-large" style="height: 34px;"/>

                <input type="text" name="rentestimate[location]" placeholder="Zip code" class="input-large" style="height: 34px;"/>
                <br>
                <button class="btn btn-primary btn-large" type="submit">Get Estimate</button>

              <% end %>

            </div>

          </div>

        </div>

      </div>
      
    </div>

  </body>


</html>

<!--%= render 'form' %-->